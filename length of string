#include <stdio.h>

int stringLength(const char *str) {
    int length = 0;
    while (*str != '\0') {
        length++;
        str++;
    }
    return length;
}

int main() {
    char str[] = "Saveetha";
    int length = stringLength(str);
    printf("The length of the string %s is %d\n", str, length);
    return 0;
}
